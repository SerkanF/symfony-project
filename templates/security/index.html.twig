{% extends 'base.html.twig' %}

{% block body %}

    <h1>Hello world</h1>

    {{ form_start(form) }}

    {{ form_row(form.email, {'required': 'false', 'attr' : { 'class' : 'myClass', 'placeholder' : 'toto' }}   ) }}

    {{ form_row(form.userName) }}

    {{ form_row(form.password) }}

    <button type="submit">Register</button>

    {{ form_end(form) }}

{% endblock %}
